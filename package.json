{
  "name": "flowbit",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
"build": "turbo run build --filter=./apps/web --filter=./apps/api",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.553.0",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.1"
}
